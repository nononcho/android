<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SimpleCalcActivity" />
        <activity android:name=".join.JoinActivity" />
        <activity android:name=".join.JoinResultActivity" />
        <activity android:name=".retrofit.AjaxListActivity" />
        <activity android:name=".ImageViewActivity" />
        <activity android:name=".ImageListActivity" />
        <activity android:name=".sql.SqlActivity" />
        <activity android:name=".sql.SqlListActivity" />
        <activity android:name=".sql.SqlModActivity" />
        <activity
            android:name=".drawer.DrawerMenuActivity"
            android:label="@string/title_activity_drawer_menu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".map.StaticMapActivity"></activity>
    </application>

</manifest>